{
	"name": "pint/limit-to-pages",
	"description": "Generate automatically \"limitToPages\" configuration.",
	"type": "typo3-cms-extension",
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "Eric Chavaillaz",
			"email": "eric.chavaillaz@gmail.com",
			"role": "Developer"
		}
	],
	"support": {
		"email": "eric.chavaillaz@gmail.com",
		"issues": "https://github.com/echavaillaz/limit_to_pages/issues",
		"source": "https://github.com/echavaillaz/limit_to_pages"
	},
	"require": {
		"ext-pdo": "*",
		"php": "^7.4",
		"typo3/cms-core": "^10.4"
	},
	"require-dev": {
		"friendsofphp/php-cs-fixer": "^3.9",
		"roave/security-advisories": "dev-latest"
	},
	"autoload": {
		"psr-4": {
			"Pint\\LimitToPages\\": "Classes/"
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "limit_to_pages"
		}
	}
}
